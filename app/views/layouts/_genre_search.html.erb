<div class="col-xs-2 genre_search">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th class="h4">ジャンル検索</th>
      </tr>
    </thead>
      <tbody>
      <% products.each do |product| %>
        <% if product.genre.is_valid == true %>
        <tr>
          <td>
            <%= link_to admin_genre_path(product.genre.id), method: :get do %>
              <%= product.genre.name %>
            <% end %>
          </td>
        </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
